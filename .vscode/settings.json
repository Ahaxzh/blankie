{
  "gpt-context-generator.detectedFileExtensions": [
    "json",
    "swift",
    "plist",
    "pbxproj",
    "entitlements",
    "md",
    "yml",
    "html",
    "astro",
    "ts",
    "mjs",
    "scss",
    "CNAME",
    "Dockerfile",
    "xcconfig",
