{
  "[swift]": {
    "editor.defaultFormatter": "vknabel.vscode-apple-swift-format",
    "editor.formatOnSave": true
  },
  "markdownlint.config": {
    "default": true,
    "MD033": false,
    "MD041": false,
  },
  "cSpell.words": [
    "aybarsnazlica",
    "blanki",
    "blanki8n",
    "Blankie",
    "Chuskas",
    "codybrom",
    "davnr",
    "Deutsch",
    "Español",
    "facebookexternalhit",
    "Français",
    "healthcheck",
    "Italiano",
    "kofi",
    "Mardojai",
    "playform",
    "Türkçe",
    "Twitterbot",
    "xcconfig",
    "xcstrings",
    "yournamehere"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": "always"
  },
  "swift.diagnostics": true,
  "swift.sourcekit-lsp.disable": true,
  "docker.lsp.experimental.vulnerabilityScanning": false,
}